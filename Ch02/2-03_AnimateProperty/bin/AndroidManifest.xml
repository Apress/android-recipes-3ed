<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.examples.animateproperty"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="12" android:targetSdkVersion="19" />

    <application
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme">
        <activity android:name=".Flipper"
            android:label="Flipper">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FlipperPause"
            android:label="FlipperPause">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
